{
    "manifest_version": 3,
    "name": "PatrickBoo Remover",
    "version": "1.0",
    "description": "Removes all elements containing data-emote-name='PatrickBoo'.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "48": "icon.png"
    }
  }
  