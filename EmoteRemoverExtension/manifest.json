{
    "manifest_version": 3,
    "name": "Emote Remover",
    "version": "1.0",
    "description": "Removes emotes from chat",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "48": "icon.png"
    }
  }
  